<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Budgeta Tests" parallel="tests" thread-count="1"
	time-out="2000000" verbose="3">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>


	<test name="Account Settings Test">
		<classes>
			<class name="com.budgeta.test.accountSettings.AccountSettingsTest"></class>
		</classes>
	</test>

	<test name="Create Budget Test">
		<classes>
			<class name="com.budgeta.test.createBudget.CreateBudgetTest"></class>
		</classes>
	</test>


</suite>













