<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Budgeta Tests" parallel="tests" thread-count="4"
	verbose="3">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="Login Test">
		<classes>
			<class name="com.budgeta.test.login.LoginTest"></class>
		</classes>
	</test>

	<test name="Scenarios Test">
		<classes>
			<class name="com.budgeta.test.scenarios.ScenariosTest"></class>
		</classes>
	</test>

	<test name="Versions Test">
		<classes>
			<class name="com.budgeta.test.versions.VersionsTest"></class>
		</classes>
	</test>

	<test name="Restore Budget Test">
		<classes>
			<class name="com.budgeta.test.reporter.ReporterTest"></class>
		</classes>
	</test>


	<test name="SignUp Test">
		<classes>
			<class name="com.budgeta.test.SignUp.SignUpTest"></class>
		</classes>
	</test>


	<test name="View Test">
		<classes>
			<class name="com.budgeta.test.view.ViewTest"></class>
		</classes>
	</test>

	<test name="Account Settings Test">
		<classes>
			<class name="com.budgeta.test.accountSettings.AccountSettingsTest"></class>
		</classes>
	</test>

	<test name="Create Budget Test">
		<classes>
			<class name="com.budgeta.test.createBudget.CreateBudgetTest"></class>
		</classes>
	</test>

	<test name="Budget Line Settings Test">
		<classes>
			<class name="com.budgeta.test.BudgetLineSettings.BudgetLineSettingsTest"></class>
		</classes>
	</test>

	<test name="Budgeta Structure Test">
		<classes>
			<class name="com.budgeta.test.budgetaStructure.BudgetaStructureTest"></class>
		</classes>
	</test>
	
	<test name="Budgeta Structure Test Part2">
		<classes>
			<class name="com.budgeta.test.budgetaStructure.BudgetaStructureTest_Part2"></class>
		</classes>
	</test>
	
	<test name="Budgeta Structure Test Part3">
		<classes>
			<class name="com.budgeta.test.budgetaStructure.BudgetaStructureTest_Part3"></class>
		</classes>
	</test>

	<test name="Budgeta Settings">
		<classes>
			<class name="com.budgeta.test.BudgetSettings.BudgetSettingsTest"></class>
		</classes>
	</test>
	
	<test name="Budgeta Error test">
		<classes>
			<class name="com.budgeta.test.RunErrorTest"></class>
		</classes>
	</test>
	
</suite>













